    "import torch\n",
    "import torch.nn as nn\n",
    "\n",
    "# 初始化数据\n",
    "input = torch.randn(3, 5, requires_grad=True)  # 预测值，随机生成\n",
    "target = torch.randn(3, 5)  # 实际目标值，随机生成\n",
    "\n",
    "# 初始化 MSELoss 对象\n",
    "mse_loss = nn.MSELoss()\n",
    "\n",
    "# 计算损失\n",
    "loss = mse_loss(input, target)\n",
    "\n",
    "# 打印损失值\n",
    "print(loss.item())\n",
    "\n",
    "# 反向传播\n",
    "loss.backward()\n",
    "\n",
    "# 此时可以更新模型参数（例如，使用优化器）\n"
   ]
